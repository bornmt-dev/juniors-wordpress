}

@media(max-width: 880px) {
    .elbzotech-mailchimp-global-style2 .content-popup-mailchimp .title{
        font-size: 30px;
        line-height: 36px;
    }
}

@media(max-width: 767px) {
    .elbzotech-mailchimp-global-style2 .info-mailchimp .desc{
