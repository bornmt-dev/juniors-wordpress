    }

    .elbzotech-mailchimp-global-style2 .info-mailchimp {
        width: 100%;
        padding: 30px
    }
    .elbzotech-mailchimp-global-style2 .elbzotech-close-popup{
        top: 5px;
        right: 5px;
    }
    .elbzotech-mailchimp-global-style2 .content-popup-mailchimp .title{
        font-size: 26px;
        line-height: 32px;
    }
}

